{
  "name": "js-dev-env",
  "version": "1.0.0",
  "description": "JavaScript Development Environment (Pluralsight)",
  "scripts": {
    "clean-dist": "rimraf ./dist && mkdir dist",
    "build": "babel-node buildScripts/build.js",
    "deploy": "surge ./dist",
    "generate-mock-data": "babel-node buildScripts/generateMockData",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "localtunnel": "lt --port 3000 --subdomain lleseg",
    "open:src": "babel-node buildScripts/srcServer.js",
    "postbuild": "babel-node buildScripts/distServer.js",
    "prebuild": "npm-run-all clean-dist test lint",
    "prestart": "babel-node buildScripts/startMessage.js",
    "prestart-mockapi": "npm run generate-mock-data",
    "share": "npm-run-all --parallel open:src localtunnel",
    "start": "npm-run-all --parallel open:src lint:watch test:watch start-mockapi",
    "start-mockapi": "json-server --watch src/api/db.json --port 3050",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Franco De Nardo",
  "license": "MIT",
  "dependencies": {
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-preset-latest": "6.16.0",
    "babel-register": "6.16.3",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "compression": "^1.7.4",
    "cross-env": "3.1.3",
    "css-loader": "^3.0.0",
    "eslint": "3.8.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-watch": "^5.1.2",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "1.0.1",
    "faker": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "9.8.0",
    "json-schema-faker": "^0.5.0-rc16",
    "json-server": "^0.15.0",
    "localtunnel": "^1.9.2",
    "mini-css-extract-plugin": "^0.7.0",
    "mocha": "^6.1.4",
    "nock": "^10.0.6",
    "npm-run-all": "3.1.1",
    "numeral": "1.5.3",
    "open": "^6.4.0",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "surge": "0.20.4",
    "webpack": "^4.35.3",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.0",
    "webpack-md5-hash": "0.0.6"
  }
}
